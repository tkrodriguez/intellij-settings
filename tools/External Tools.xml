<toolSet name="External Tools">
  <tool name="mx build" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="mx" />
      <option name="PARAMETERS" value="build" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
    <filter>
      <option name="NAME" value="No name" />
      <option name="DESCRIPTION" />
      <option name="REGEXP" value="$FILE_PATH$:$LINE$" />
    </filter>
  </tool>
  <tool name="mx libgraal build" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="mx" />
      <option name="PARAMETERS" value="--env libgraal --extra-image-builder-argument=-ea --extra-image-builder-argument=-J-esa build" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$/../../graal/vm" />
    </exec>
    <filter>
      <option name="NAME" value="No name" />
      <option name="DESCRIPTION" />
      <option name="REGEXP" value="$FILE_PATH$:$LINE$" />
    </filter>
  </tool>
  <tool name="mx compiler style checks" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="mxgatestyle" />
      <option name="PARAMETERS" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$/../../graal/compiler" />
    </exec>
    <filter>
      <option name="NAME" value="No name" />
      <option name="DESCRIPTION" />
      <option name="REGEXP" value="$FILE_PATH$:$LINE$.*" />
    </filter>
    <filter>
      <option name="NAME" value="No name" />
      <option name="DESCRIPTION" />
      <option name="REGEXP" value="$FILE_PATH$ .at line $LINE$.*" />
    </filter>
  </tool>
  <tool name="mx enterprise style checks" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="mxgatestyle" />
      <option name="PARAMETERS" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$/../../graal-enterprise/graal-enterprise" />
    </exec>
    <filter>
      <option name="NAME" value="No name" />
      <option name="DESCRIPTION" />
      <option name="REGEXP" value="$FILE_PATH$:$LINE$.*" />
    </filter>
    <filter>
      <option name="NAME" value="No name" />
      <option name="DESCRIPTION" />
      <option name="REGEXP" value="$FILE_PATH$ .at line $LINE$.*" />
    </filter>
  </tool>
</toolSet>